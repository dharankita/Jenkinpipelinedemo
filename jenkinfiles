pipeline {
    agent any

    stages {
        stage('code') {
            steps {
                echo 'Coding'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        }
        }
    

